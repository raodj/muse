# This is also a comment
epidemic testing {
   constants {
      pi = 3.142;  # Comments can be *almost* anywhere
      e  = 2.718;
   }

   parameters {
       beta, mu, psi, omega; # These are parameters passed-in to the kernel
   }

    states {
        s, e, i, r;  # These are compartments and are part of the state.
    }

    transitions {
       s += +1          @ n * mu;
       s += -1, e += 1  @ s * beta;
       i += +1, e += -1 @ e * psi;
       e += -1, r += 1  @ i * omega; 
    }
}
